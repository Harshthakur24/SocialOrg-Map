{
  "extends": [
    "next/core-web-vitals",
    "next/typescript"
  ],
  "rules": {
    "no-unused-vars": "warn", // Change to 'warn' to avoid deployment failures
    "react/no-children-prop": "off", // Disable if you are using children as a prop
    "no-console": "warn", // Change to 'warn' to allow console statements in development
    "@typescript-eslint/no-empty-interface": "off" // Disable the empty interface rule if needed
  }
}
